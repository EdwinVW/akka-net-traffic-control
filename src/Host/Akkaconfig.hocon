akka 
{
	stdout-loglevel = ERROR
	loglevel = ERROR
	log-config-on-start = on   

	actor 
	{
		debug 
		{  
			  receive = on 
			  autoreceive = on
			  lifecycle = on
			  event-stream = on
			  unhandled = on
		}

		provider = remote

		deployment 
		{
			/cjcaactor 
			{
				remote = "akka.tcp://cjcasystem@localhost:9999"
			}
		}
	}

	remote 
	{
		dot-netty.tcp 
		{
			port = 0 
			hostname = localhost
		}
	}
}
